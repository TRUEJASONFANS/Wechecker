package soot.we.android.IntentResolution;

import java.util.HashSet;
import java.util.Set;
/**
 * The entrypoint component(which is exported) will be the rootnode of the InvokeGraph;
 * @author jason
 *
 */
public class ComponentInvokeGraph {
	public Set<ComponentInvokeGraphNode> roots;
	public int deep;
	public ComponentInvokeGraph(){
		roots = new HashSet<ComponentInvokeGraphNode>();
	}
	public void addRootTreeNode(ComponentInvokeGraphNode r) {
		roots.add(r);
	}
	
}

